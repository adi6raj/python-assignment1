    "import pickle\n",
    "\n",
    "# A Python object (dictionary in this case)\n",
    "data = {\"name\": \"Alice\", \"age\": 25, \"city\": \"New York\"}\n",
    "\n",
    "with open(\"data.pickle\",\"wb\") as f:\n",
    "    pickle.dump(data, f)\n",
    "\n",
    "print(\"Data has been serialized and saved to data.pickle\")"
